{
  "name": "london-towers",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently --kill-others --names \"JS,CSS\" -c \"bgBlue.bold,bgGreen.bold\" \"yarn dev:js\" \"yarn css\"",
    "css": "cross-env TAILWIND_MODE=watch yarn postcss styles/main.css -o src/compiled.css -v --watch",
    "dev:js": "node server.js",
    "prebuild": "yarn postcss styles/main.css -o src/compiled.css -v",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "next": "12.1.6",
    "react": "18.1.0",
    "react-dom": "18.1.0"
  },
  "devDependencies": {
    "@types/node": "17.0.40",
    "@types/react": "18.0.11",
    "@types/react-dom": "18.0.5",
    "autoprefixer": "^10.4.7",
    "concurrently": "^7.2.1",
    "cross-env": "^7.0.3",
    "cssnano": "^5.1.11",
    "eslint": "8.17.0",
    "eslint-config-next": "12.1.6",
    "postcss": "^8.4.14",
    "postcss-cli": "^9.1.0",
    "postcss-import": "^14.1.0",
    "postcss-nested": "^5.0.6",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "tailwindcss": "^3.0.24",
    "typescript": "4.7.3"
  }
}
